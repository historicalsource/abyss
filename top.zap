?CCL13:	ZERO?	WD2 /FALSE
	INTBL?	WD2,NP,CT \?CCL18
	RETURN	WD2
?CCL18:	ZERO?	WD3 /FALSE
	INTBL?	WD3,NP,CT \FALSE
	RETURN	WD3
?CCL10:	EQUAL?	WD1,FALSE-VALUE /TRUE
	RFALSE	
?CCL3:	EQUAL?	WD1,FALSE-VALUE /TRUE
	RFALSE	

