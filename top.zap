	.FUNCT	ADJ-USED?:ANY:2:4,OBJ,WD1,WD2,WD3,NP,CT
	CALL2	GET-NP,OBJ >NP
	GET	NP,1 >NP
	ZERO?	NP /?CCL3
	GET	NP,2
	EQUAL?	PLAYER,STACK \?CCL6
